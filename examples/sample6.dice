func wait5() {
    wait(0.5);
    print("wait5の終了");
}

func wait10() {
    wait(1);
    print("wait10の終了");
}

func wait20() {
    wait(2);
    print("wait20の終了");
}

func main() {
    bool_value = 10 == 1;

    parallel {
        if (bool_value) {
            @timed
            wait5();
        }
        else {
            @timed
            wait10();
        }
        @timed
        wait20();
    }
}